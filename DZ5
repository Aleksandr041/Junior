using System;

namespace ConsoleDZ5
{
    class Program
    {
        static void Main(string[] args)
        {
            string surname = "Петр";
            string name = "Петров";
            string bufferName;

            Console.WriteLine("" + surname + " " + name);

            bufferName = surname;
            surname = name;
            name = bufferName;

            Console.WriteLine("" + surname + " " + name);
        }
    }
}
