using System;

namespace Practice12_2
{
    class Program
    {
        static void Main(string[] args)
        {
            int picturesRow = 3;    
            int totalPictures = 52;
            int totalRows;
            int picturesOutsideRow;

            totalRows = totalPictures / picturesRow;
            picturesOutsideRow = totalPictures % picturesRow;

            Console.WriteLine($"Полностью заполненных рядов {totalRows}. Картинки сверх меры {picturesOutsideRow}.");
        }
    }
}
